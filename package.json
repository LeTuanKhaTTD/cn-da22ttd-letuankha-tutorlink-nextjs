{
  "name": "tutorlink-tvu",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Hệ thống kết nối gia sư sinh viên TVU với phụ huynh",
  "author": "Lê Tuấn Khá <110122086@sv.tvu.edu.vn>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\" --names \"SERVER,CLIENT\" --prefix-colors \"bgBlue,bgGreen\"",
    "dev:frontend": "vite",
    "dev:backend": "node backend/server.js",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "server": "node backend/server.js",
    "server:dev": "nodemon backend/server.js",
    "check": "node backend/quick-check.js",
    "setup": "node backend/auto-setup-complete.js",
    "check:full": "node backend/check-system.js"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.3",
    "openai": "^6.15.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
